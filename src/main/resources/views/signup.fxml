<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="859.0" prefWidth="1035.0" styleClass="anchor-pane" stylesheets="@../css/background.css"
            xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.peerapplication.controller.SignupController">
    <children>
        <AnchorPane layoutX="313.0" layoutY="205.0" prefHeight="449.0" prefWidth="409.0"
                    style="-fx-background-color: #dcdcdc;" AnchorPane.bottomAnchor="205.0" AnchorPane.leftAnchor="313.0"
                    AnchorPane.rightAnchor="313.0" AnchorPane.topAnchor="205.0">
            <children>
                <TextField fx:id="txtUsername" layoutX="79.0" layoutY="154.0" prefHeight="33.0" prefWidth="243.0"
                           promptText="Username" stylesheets="@../css/textbox.css"/>
                <Button fx:id="btnSignup" layoutX="79.0" layoutY="327.0" mnemonicParsing="false"
                        onMouseClicked="#confirmSignup" prefHeight="33.0" prefWidth="243.0"
                        stylesheets="@../css/button.css" text="Sign Up"/>
                <Button fx:id="btnLogin" layoutX="79.0" layoutY="377.0" mnemonicParsing="false"
                        onMouseClicked="#openLogin" prefHeight="33.0" prefWidth="243.0" stylesheets="@../css/button.css"

                <Label fx:id="statusLabel" layoutX="79.0" layoutY="110.0" prefHeight="22.0" prefWidth="243.0"/>
                <ImageView fitHeight="117.0" fitWidth="129.0" layoutX="142.0" layoutY="-58.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../images/iwojqaylqxslcnbpnpwd.png"/>
                    </image>
                </ImageView>
                <Label alignment="CENTER" layoutX="145.0" layoutY="68.0" prefHeight="35.0" prefWidth="111.0"
                       style="-fx-text-fill: #6495ed;" text="Sign Up" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="24.0"/>
                    </font>
                </Label>
                <PasswordField fx:id="txtPassword" layoutX="79.0" layoutY="213.0" prefHeight="33.0" prefWidth="243.0"
                               promptText="Password" stylesheets="@../css/textbox.css"/>
                <PasswordField fx:id="txtCnfmPassword" layoutX="79.0" layoutY="269.0" prefHeight="33.0"
                               prefWidth="243.0" promptText="Confirm Password" stylesheets="@../css/textbox.css"/>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>

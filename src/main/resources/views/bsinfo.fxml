<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="248.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.peerapplication.controller.BSInfoController">
    <children>
        <TextField fx:id="txtBSPort" layoutX="82.0" layoutY="135.0" prefHeight="25.0" prefWidth="219.0"
                   promptText="Port"/>
        <TextField fx:id="txtBSIP" layoutX="82.0" layoutY="93.0" prefHeight="25.0" prefWidth="219.0"
                   promptText="IP Address"/>
        <Button fx:id="btnConnect" layoutX="135.0" layoutY="179.0" mnemonicParsing="false" onMouseClicked="#connect"
                prefHeight="25.0" prefWidth="112.0" text="Connect"/>
        <Label fx:id="lblStatus" layoutX="82.0" layoutY="56.0" prefHeight="17.0" prefWidth="219.0"
               text="Provide Bootrstrap Server IP and Port." textFill="#da0c0c"/>
    </children>
</AnchorPane>
